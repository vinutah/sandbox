   "source": [
    "def factorial(n):\n",
    "    if n <= 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "import ipywidgets as wdg\n",
    "wdg.interact(factorial, n=2)"
   ]
